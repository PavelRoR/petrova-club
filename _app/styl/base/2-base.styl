*
	margin 0
	padding 0
	outline none
body
	font-size: 20px
	font-family: C
	color #fff
	background-color #1B0A08
	line-height: 1.1
	@media screen and (max-width 560px)
		font-size: 16px
.wrapper
	background: #1B0A08 url(../img/backs/back-1.png) no-repeat top center
	background-size: cover
	overflow hidden
	position relative
	@media screen and (max-width 991px)
		background-image: url(../img/backs/back-2.png)
	@media screen and (max-width 440px)
		background-image: url(../img/backs/back-mob.png)
.content
	position relative
	z-index 60
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	// z-index 60
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
p
	margin 0
	font-size: 30px
	@media screen and (max-width 1199px)
		font-size: 25px
	@media screen and (max-width 560px)
		font-size: 20px
