.general-title
	font-family: D
	font-size: 45px
	text-transform uppercase
	margin-bottom: 80px
	text-align center
	@media screen and (max-width 1199px)
		font-size: 32px
		margin-bottom: 60px
	@media screen and (max-width 560px)
		font-size: 23px
		margin-bottom: 45px
.general-list
	// padding-left: 30px
	list-style none
	li
		font-size: 30px
		position relative
		padding-left: 25px
		margin-bottom: 5px
		@media screen and (max-width 1199px)
			font-size: 25px
			padding-left: 20px
		@media screen and (max-width 560px)
			font-size: 20px
		&:before
			content: ''
			display block
			position absolute
			top 14px
			left 0
			border-radius: 50%
			width 6px
			height: 6px
			background: #fff
			@media screen and (max-width 1199px)
				top 12px
			@media screen and (max-width 560px)
				top 10px
				width 4px
				height: 4px
		// margin-bottom: 10px
		// &:last-child
		// 	margin-bottom: 0
.d-none
	display none !important
	@media screen and (max-width 991px)
		display block !important
.mob-none
	@media screen and (max-width 991px)
		display none